{
  "name": "@churchcommunitybuilder/vector-icons",
  "version": "0.11.0",
  "description": "Vector Icons",
  "author": "Alex Beckwith <alfonz129@gmail.com>",
  "license": "MIT",
  "repository": "github:churchcommunitybuilder/vector-icons",
  "main": "dist/index.js",
  "files": [
    "/dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
