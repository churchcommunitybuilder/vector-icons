{
  "name": "@churchcommunitybuilder/vector-icons",
  "version": "0.3.0",
  "description": "Vector Icons",
  "repository": "https://github.com/churchcommunitybuilder/vector-icons.git",
  "author": "Alex Beckwith <alfonz129@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "format": "yarn prettier src/**/*.{ts,tsx}",
    "build": "rm -rf dist && yarn tsc -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "postpublish": "cd .. && yarn expo p"
  },
  "files": [
    "/dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "react": "16.8.6"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react-native-svg": "^9.5.1"
  }
}
